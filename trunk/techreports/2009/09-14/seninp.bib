    month = feb,
    number = {2},
    pages = {306--318},
    posted-at = {2009-02-07 14:07:24},
    priority = {2},
    publisher = {IEEE},
    title = {Time Warp Edit Distance with Stiffness Adjustment for Time Series Matching},
