    number = {3},
    pages = {794--805},
    posted-at = {2010-06-03 16:12:20},
    priority = {2},
    publisher = {Ecological Society of America},
    title = {Uncertainty in Spatially Explicit Animal Dispersal Models},
    url = {http://dx.doi.org/10.2307/4134696},
    volume = {13},
