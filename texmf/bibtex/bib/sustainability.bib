@article{saunders-1992,
	Abstract = {Focuses on the neoclassical growth theory in the electricity industry of the United States, looking at research carried out by Daniel Khazzoom and Leonard Brookes. Relation between energy efficiency and energy demand; Information on the postulate by Khazzoom and Brookes; Research carried out by other scientists; Information on the autonomous energy efficiency index; Details on the neoclassical growth theory.},
