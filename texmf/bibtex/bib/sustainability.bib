@article{Young:2000fv,
	Abstract = {This article contends that while striving to promote environmentally responsible behavior, we have focused attention too narrowly on just two classes of motives. There is a need to expand the range of motives available to practitioners and to provide a framework within which motives can be evaluated for both their immediate and long-term effectiveness. The article then examines a strategy for promoting environmentally responsible behavior that has significant potential. This strategy is based on a particular form of motivation called intrinsic satisfaction. Nine studies are reviewed that have outlined the structure of intrinsic satisfaction. A key theme discussed is the human inclination for competence. This fundamental human concern is shown to have both a general form and a resource-specific version.},
	Annote = {\begin{itemize}
\item Motives pursued for environmentally responsible behavior (ERB) constrained to material incentives/disincentives and altruistic reasons
\item ``incentives needed constant reintroduction to remain effective and they proved to be less reliable than we had hoped''
\item Altruism as motive suffers from lack of guidelines for actual use
\item ERB is a result of multiple motives, not just one
\item Two types of evaluation criteria: outcome-based (technique in isolation), and context-based (effect of other factors on technique)
\item Cone and Hayes 1980: ``(a) whether a technique can be reliably implemented by a variety of individuals and (b)its ability to promote durable behavior change''
\item incentives can start ERB, but behavior changes back when incentives end, and even continuing incentives can have low reliability
\item ``psychological reactance, where the recipient does the opposite of what is demanded''. In some circumstances, people do the opposite of the ERB you are asking them to undertake
\item big behavior changes can change the initiators of the change: ``contempt for those 
people he is influencing and self-contempt''
\item The other criterion is the generalizability of a motivational approach to ``other environmental problems, settings, and contexts''
\item Evidence that recycling in the home is predictive of recycling in the office
\item conceptual familiarity might be enough, not just direct experiences
\item Motivators can be more universal (money) or less universal (personal attention)
\item (RB unsurprisingly) ``Motives will be more effective in those instances in which the 
behavior the motive seeks to promote goes to the core of a person's needs or concerns.'' 
\item self-interest generally considered cause of environmental problems: ``focusing 
solely on short-term individual or familial gain to the exclusion of long-term societal or environmental benefits''
\item thesis: self-interest can be a solution to environmental problems
\item self-interest is different from selfishness, self-interest meaning each individual is responsible for getting their own needs met
\item ``although happiness is experienced personally, it is derived from attaining an outcome, any outcome, we care about.'' So improving an ecosystem can make us happy
\item intrinsic satisfaction: ``certain patterns of behavior are worth engaging in because of the personal, internal contentment that engaging in these behaviors provides.''
\item Based on 9 different studies of ERB, author finds 3 intrinsic satisfactions:
	\begin{itemize}
	\item 1. satisfaction derived from striving for behavioral competence
	\item 2. frugal, thoughtful consumption
	\item 3. participation in maintaining a community
	\end{itemize}
\item Competence, enjoyment in completing tasks and solving problems
\item Frugality, enjoyment from ``careful stewardship of finite resources''
\item Participation, enjoyment from participating in community activities. ``eagerness to share news, finding pleasure from working with others toward a common goal''
\item Tested luxury, since that is generally considered the opposite of conservation. Found that participants ``did not view satisfaction derived from luxury as the antithesis of satisfaction gained from the other behavioral patterns''
\item While attitudes and norms can cause behavior change, you also need skills and experience with the change
\item ``Without considering these variables, we make the error of assuming that once people know what they should do and why they should do it, they will automatically know how to proceed.''
\item When using competence as a motivator, need to pay attention to context. People don't like it when they ``cannot advance or utilize their competence''
\item Way to use competence: ``providing a context in which procedural information is readily available and behavior can tentatively be tried in a supportive environment''

\end{itemize}},
	Author = {Raymond De Young},
	Date-Added = {2008-10-24 15:56:15 -1000},
	Date-Modified = {2008-11-16 20:05:32 -1000},
	Doi = {10.1111/0022-4537.00181},
	Journal = {Journal of Social Issues},
	Number = {3},
	Pages = {509--526},
	Rating = {4},
	Read = {Yes},
	Title = {Expanding and Evaluating Motives for Environmentally Responsible Behavior},
	Url = {http://dx.doi.org/10.1111/0022-4537.00181},
	Volume = {56},
	Year = {2000},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1111/0022-4537.00181}}

@techreport{darby-review-2006,
	Abstract = {Most householders have only a vague idea of how much energy they are using for different purposes, hence the importance of making energy flows more visible and controllable. There is a lot of interest in the potential for better feedback using improved ('smart') metering, more informative billing and direct display panels. This review considers what is known about the effectiveness of different types of feedback, with a focus on how people change their behaviour.},
	Annote = {\begin{itemize}
\item Most people don't know how much energy they use, and thus how they could reduce energy usage
\item Feedback allows the user to learn through experimentation
\item Feedback combined with frequent billing is even better
\item Direct feedback usually leads to energy savings of 5-15%
\item ``high energy users may respond more than low users to direct feedback.''
\item Indirect feedback (billing) is better for assessing impacts in efficiency measures, can save 0-10%
\item ``Historic feedback appears to be more effective than comparative or normative''
\item Feedback down to the appliance level is difficult to supply, but users can figure it out from instantaneous feedback
\item Pay-as-you-go electricity can lead to 10-20% savings
\item Online billing is not a substitute for direct display
\item Persistence of savings happens when feedback has led to new habits or undertaking efficiency measures
\item Incentives to save energy lead to savings that disappear when incentives are removed
\item Behavior formed over a 3 month period is more likely to persist than shorter periods
\item ``Energy supply and consumption are sociotechnical in nature: technology and behaviour interact and co-evolve with each other over time.''
\item ``Consumption in identical homes, even those designed to be low-energy dwellings, can easily differ by a factor of two or more depending on the behaviour of the inhabitants''
\item Traditional billing sucks for decision making
\item feedback types: direct (displays, ambient devices), indirect (bills), inadvertent feedback due to microgeneration, utility-controlled feedback (smart meters), energy audits
\item feedback helps build tacit knowledge about supply and use of energy
\item savings from feedback varies depending on technology, context, etc
\item just looking at a standard meter occasionally can help, but not as much as instant feedback
\item Direct feedback on displays leads to savings on the order of 10%
\item Users found pie charts of typical breakdowns of home energy use helpful, even though they were averages as opposed to being drawn from the user's data
\item Simple ambient display (flashing light when temperature outside is below 68 to inform users to turn off air conditioner) provided savings of 16\%
\item Apparently in the UK your meter might only be read once every 2 years, with the bills being an estimate of usage. Yikes!
\item Users might like to see comparative information, but it doesn't necessarily lead to savings. Also, the problem of showing that you are saving more than your comparable group could lead to increased consumption
\item Internal motivation is most important for maintaining savings
\item For people doing microgeneration (like PV) display of energy in and energy out can lead to reduction in electricity consumption by 20\%
\item ``synergies between different types of feedback and between feedback and other types of information.''
\item During CA energy crisis, LBNL made a website that showed current electricity load, capacity, and forecasts for usage. Anecdotal evidence that people changed their usage based on the graphs
\end{itemize}},
	Author = {Sarah Darby},
	Date-Added = {2008-10-24 15:40:54 -1000},
	Date-Modified = {2008-11-12 18:26:41 -1000},
	Institution = {Environmental Change Institute, University of Oxford},
	Rating = {5},
	Read = {Yes},
	Title = {The effectiveness of feedback on energy consumption},
	Year = {2006},
	Bdsk-File-1 = {},
	Bdsk-File-2 = {},
	Bdsk-Url-1 = {http://www.eci.ox.ac.uk/research/energy/electric-metering.php}}

@article{Skip-Laitner:2000yg,
	Abstract = { Recent controversy suggests that energy efficiency policies used to reduce carbon emissions might actually increase overall energy consumption. The result would be an unintended increase in carbon emissions. This paper examines the underlying issues of this so-called "rebound effect" from both a historical perspective and through the results of a recent macroeconomic analysis completed for the United States. Depending on the assumptions of income and price elasticities, as well as the supply/demand interactions within a macroeconomic model, the rebound effect might reduce overall savings by about 2-3% compared to a pure engineering analysis. In other words, an economy-wide, cost-effective engineering savings of 30% might turn out to be only a 29% savings from a macroeconomic perspective. Despite the impact of a rebound effect, the net result of energy efficiency policies can be a highly positive one.},
	Annote = {\begin{itemize}
\item Question: does energy efficiency save energy at the macro level, or does it encourage greater use?
\item If true, then energy companies should lobby heavily for efficiency measures
\item Khazzoom introduced rebound effect: because efficiency reduces cost of energy, which offsets some of the savings
\item Brookes suggested "if reductions in energy use per unit of output are cost-effective, then such reductions would lead to increases rather than decreases in energy use within the larger economy."
\item US energy intensity decreased since 1973 (energy per dollar GDP)
\item reduction due to energy efficiencies and structural changes in economy (more service-based than manufacturing)
\item However overall consumption is rising (78.4 exajoules in 1973 and 100 exajoules in 1998)
\item "there is the very real possibility that energy efficiency investments will improve overall national income and reduce energy prices."
\item  "That greater level of income, coupled with possibly lower energy prices, may cause overall energy use to rebound at a faster clip than the rate of efficiency improvements."
\item Presents an equation that relates future carbon emissions to current carbon emissions, increases in GDP and energy costs, and elasticities of income and energy prices
\item based on the simple model, rebound effect from improved efficiency might increase emissions only 2.4\%
\item RB energy prices have increase dramatically since this paper was written, so more evidence that rebound effect is minor
\item Many people believe that reducing GHG emissions and increasing GDP are contradictory
\item However EPA/LBNL analysis shows that there substantial improvements in the economy's carbon efficiency can be made, reducing emissions while increasing GDP
\item EPA/LBNL analysis using National Energy Modeling System
showed that an "energy-efficient/low-carbon technology path" would suffer from a rebound effect of only 2.2\%
\item Reductions in energy usage due to efficiency could be undone if energy users use the reduction usage to increase consumption 
\end{itemize}},
	Author = { John A. ``Skip'' Laitner},
	Date-Added = {2008-10-24 15:21:37 -1000},
	Date-Modified = {2008-12-21 12:53:06 -1000},
	Doi = {http://dx.doi.org/10.1016/S0301-4215(00)00032-X},
	Journal = {Energy Policy},
	Keywords = {Elasticities; Macroeconomic perspectives; Energy policies},
	Number = {6-7},
	Pages = {471--475},
	Rating = {4},
	Read = {Yes},
	Title = {Energy efficiency: rebounding to a sound analytical perspective},
	Ty = {JOUR},
	Url = {http://www.sciencedirect.com/science/article/B6V2W-4090S0W-D/2/91763f710ffcd38dd30842ca42d95f59},
