	Number = {4},
	Pages = {194--203},
	Read = {No},
	Title = {Technological innovation, energy efficient design and the rebound effect},
	Ty = {JOUR},
	Url = {http://www.sciencedirect.com/science/article/B6V8B-4MR1RPP-1/2/5dfb7e999088b5f5471c114386504b27},
