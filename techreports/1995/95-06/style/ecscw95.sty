\def\abstract{\section{\footnotesize\sf Abstract}}

\def\endabstract{}

\def\section{\@startsection{section}{1}{\z@}{-24pt}{12pt}{\reset@font\Large}*}
\def\subsection{\@startsection{subsection}{2}{\z@}{-14pt}{9pt}{\reset@font\large}*}
\def\subsubsection{\@startsection{subsubsection}{3}{\z@}{-9pt}{6pt}{\reset@font\normalsize}*}

\def\quotation{\list{}{\topsep 2pt\leftmargin 5mm\footnotesize}\item[]}
\let\endquotation=\endlist

\def\quote{\list{}{\topsep 2pt\leftmargin 5mm\footnotesize}\item[]}
\let\endquote=\endlist

\def\center{\trivlist \centering\item[]\leavevmode}

\long\def\@makecaption#1#2{%
   \vskip 12pt
\footnotesize       #1. #2\par
	\vskip 12 pt
}

\def\labelenumi{(\theenumi)}

\leftmargini 10mm
\leftmarginii 15mm
\leftmarginiii 20mm

\leftmargin\leftmargini

\def\appendix{\par
  \setcounter{section}{0}
  \setcounter{subsection}{0}
  \def\thesection{}
	\footnotesize}

\def\thebibliography#1{\section{\refname\@mkboth
  {\uppercase{\refname}}{\uppercase{\refname}}}\list
  {\@biblabel{\arabic{enumiv}}}{\settowidth\labelwidth{\@biblabel{#1}}%
    \leftmargin\labelwidth
    \advance\leftmargin\labelsep
    \usecounter{enumiv}%
    \let\p@enumiv\@empty
    \def\theenumiv{\arabic{enumiv}}}%
    \def\newblock{\hskip .11em plus.33em minus.07em}%
    \sloppy\clubpenalty4000\widowpenalty4000
    \sfcode`\.=1000\relax}

