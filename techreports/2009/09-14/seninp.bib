    keywords = {dtw, litreview, thesis},
    number = {11},
    pages = {2040--2046},
    posted-at = {2008-12-01 03:49:25},
    priority = {2},
    title = {Sign Language Recognition by Combining Statistical {DTW} and Independent Classification},
