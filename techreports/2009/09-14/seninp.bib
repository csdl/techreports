    number = {6},
    pages = {623--635},
    posted-at = {2008-12-01 03:52:05},
    priority = {2},
    title = {Performance tradeoffs in dynamic time warping algorithms for isolated word recognition},
    url = {http://ieeexplore.ieee.org/xpls/abs\_all.jsp?arnumber=1163491},
    volume = {28},
    year = {1980}
