@TechReport{Crawford-Hines95,
  author = 	 "Stewart Crawford-Hines",
  title = 	 "Software Inspections and Technical Reviews: Transcending the Dogma",
  institution =  "University of Colorado, Department of Computer Science",
  year = 	 "1995",
  type = 	 "Technical Report",
  categories = {method=inspection},
  URL =  {http://www.cs.colostate.edu/~sgcraw/RandA/paper.html},
  abstract = {Historically, much of the discussion around procedural issues
     of Software Inspections and Technical Reviews has been rather
     dogmatic.  Many have defined a process in their one particular way and
     preach it as the One True Path from which no one may stray.
     However, software development organizations are as diverse as the
     people that make them up.  It should be no surprise that what works
     in one organization may be counterproductive in another.
     This report attempts to call out some of the current dogma and
     explore its roots, to provide a more balanced view in defining
     a review or inspection process appropriate for any individual
     development organization.}
}

