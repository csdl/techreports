@Article{Brykczynski95,
  author = 	 "Bill Brykczynski",
  title = 	 "The Software Inspection Process-Applying the Principles of {Deming} and {Crosby}",
  journal = 	 "Information and Systems Engineering",
  year = 	 "1995",
  volume = 	 "1",
  number = 	 "1",
  pages = 	 "23-37",
  month = 	 "March",
  abstract = 	 "Software inspection is one of the most effective approaches
used for detecting and eliminating defects throughout the software
development process. Key software inspection concepts include early
defect detection, rework reduction, causal analysis, and continuous
process improvement. These concepts are remarkably similar to several
quality improvement principles set forth by W. Edwards Deming and
Philip Crosby. In this paper we examine how the inspection process can
be used in software engineering to implement many of the principles of
Deming and Crosby. We also examine the applicability of these
principles to the general field of software engineering.",
